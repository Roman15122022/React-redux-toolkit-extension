{
  "manifest_version": 3,
  "name": "TrackerR",
  "description": "It is extension for tracking time",
  "version": "1.2.8",
  "action": {
    "default_popup": "popup.html",
    "default_title": "TrackerR",
    "default_icon": "icon.png"
  },
  "permissions": ["storage", "notifications", "alarms"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
