{
  "manifest_version": 3,
  "name": "TrackerR",
  "description": "It is extension for tracking time",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "React Boilerplate",
    "default_icon": "icon.png"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html"
}
